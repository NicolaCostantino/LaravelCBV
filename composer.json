{
    "name": "nicolacostantino/laravelcbv",
    "description": "A package implementing Django's Class-Based Views (CBV) for Laravel. It keeps a 1:1 mapping with Django 2.* classes, methods behavior and naming",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "^5.6",
        "nicolacostantino/seven": "v1.0.0"
    },
    "autoload": {
        "psr-4": {
            "NicolaCostantino\\LaravelCBV\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NicolaCostantino\\LaravelCBV\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nicola Costantino",
            "homepage": "https://nicolacostantino.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "NicolaCostantino\\LaravelCBV\\CBVServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/NicolaCostantino/Seven"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^3.7@dev",
        "phpunit/phpunit": "^7.3@dev || ^8.0",
        "orchestra/testbench-browser-kit": "~3.1",
        "mockery/mockery": "dev-master"
    }
}
